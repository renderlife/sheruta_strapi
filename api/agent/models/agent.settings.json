{
  "kind": "collectionType",
  "collectionName": "agents",
  "info": {
    "name": "agent",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 30,
      "minLength": 3,
      "unique": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "logo_url": {
      "type": "text",
      "default": "https://www.welcome-service.ch/wp-content/uploads/2014/11/company-icon-300.png",
      "required": true,
      "minLength": 5
    },
    "phone_number": {
      "type": "string",
      "required": true,
      "maxLength": 15,
      "minLength": 11,
      "private": false
    },
    "banner_url": {
      "type": "text",
      "default": "https://img.freepik.com/free-vector/green-background-with-halftone-effect-squares_23-2148661430.jpg?size=626&ext=jpg",
      "required": true
    },
    "twitter": {
      "type": "string"
    },
    "instagram": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "google_location": {
      "type": "json",
      "required": true
    },
    "user": {
      "via": "agent",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
